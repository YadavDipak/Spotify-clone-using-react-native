{
  "expo": {
    "name": "spotify-project",
    "slug": "spotify-project",
    "scheme": "spotify-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.spotifyproject",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["spotify-project"]
          }
        ],
        "LSApplicationQueriesSchemes": ["spotify-project"]
      },
      "associatedDomains": [
        "applinks:my-app-dipak.com",
        "applinks:spotify-project.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.spotifyproject",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "my-app-dipak.com",
              "pathPrefix": "/song/"
            },
            {
              "scheme": "spotify-project",
              "host": "song"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-localization"]
  }
}
